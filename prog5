package ru.mirea.lab1;


import java.util.Scanner;


public class prog5 {
    public static int get_fact(int num) {
        int fact = 1;
        for (int i = 1; i <= num; i++){
            fact *= i;
        }
    return fact;
    }

    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.println("Факториал: " + get_fact(input.nextInt()));
    }
}
